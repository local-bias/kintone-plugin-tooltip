{
  "name": "kintone-plugin-template",
  "version": "1.0.0",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "init": "plugin init && plugin genkey",
    "build": "concurrently \"npm:build:*\" && plugin zip",
    "build:js": "plugin build",
    "build:css": "tailwindcss -i ./src/styles/global.css -o ./.plugin/contents/config.css -c ./tailwind.config.config.mjs --minify && tailwindcss -i ./src/styles/global.css -o ./.plugin/contents/desktop.css -c ./tailwind.config.desktop.mjs --minify",
    "standalone": "concurrently \"npm:build:*\" && plugin zip --env standalone",
    "dev": "concurrently \"npm:dev:*\"",
    "dev:plugin": "plugin dev",
    "dev:css-config": "tailwindcss -i ./src/styles/global.css -o ./.plugin/dev/config.css -c ./tailwind.config.config.mjs -w",
    "dev:css-desktop": "tailwindcss -i ./src/styles/global.css -o ./.plugin/dev/desktop.css -c ./tailwind.config.desktop.mjs -w",
    "lint": "eslint ./src --cache --ext .js,.ts,.tsx"
  },
  "browser": {
    "path": false
  },
  "dependencies": {
    "@emoji-mart/data": "^1.2.1",
    "@emoji-mart/react": "^1.1.1",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "11.11.5",
    "@heroicons/react": "^2.1.3",
    "@konomi-app/kintone-utilities": "*",
    "@konomi-app/kintone-utilities-react": "*",
    "@konomi-app/ui-react": "*",
    "@lb-ribbit/kintone-secrets": "^0.3.0",
    "@mui/icons-material": "5.15.19",
    "@mui/material": "5.15.19",
    "emoji-mart": "^5.6.0",
    "immer": "10.1.1",
    "luxon": "3.4.4",
    "notistack": "3.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "4.0.13",
    "recoil": "0.7.7"
  },
  "devDependencies": {
    "@kintone/dts-gen": "8.0.11",
    "@kintone/plugin-uploader": "9.1.1",
    "@kintone/rest-api-client": "5.5.1",
    "@konomi-app/k2": "*",
    "@types/luxon": "3.4.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.12.0",
    "@typescript-eslint/parser": "7.12.0",
    "concurrently": "^8.2.2",
    "dotenv": "16.4.5",
    "eslint": "8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.8.1",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-react": "^7.34.2",
    "tailwindcss": "^3.4.4",
    "typescript": "5.4.5"
  }
}